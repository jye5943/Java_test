import java.util.Scanner;

public class Prof {
	public static void main(String[] args) {
	
			int a;
			int b;
			int c;
			
			Scanner scan = new Scanner(System.in);
			
			a = scan.nextInt();
			b = scan.nextInt();
			c = scan.nextInt();
			
	System.out.println ("총점은" + (a+b+c) + "입니다")"
	
	
	}
	}

	
