import java.util.Scanner;

public class Prof {
	public static void main(String[] args) {
	
			int a;
			int b;
			int c;
			int i= a+b+c
					
			Scanner scan = new Scanner(System.in);
		
			for (int i = 0; i < 100; i++) {
				a = scan.nextInt();
				
			a = scan.nextInt();
			b = scan.nextInt();
			c = scan.nextInt();
			
	System.out.println ("총점은" + (a+b+c) + "입니다");
	
	
	}
	}

	
