import java.util.Random;

public class Test11 {
public static void main(String[] args) {
	
	Random randNumber = new Random();
	
	int r1 = randNumber.nextInt(6) + 1; //6개가 발생(0~5)
 if (r1==1) {
	 System.out.println("멍멍");
	 else if (r1 == 2) {
		 System.out.println("야옹");
	 } else if (r1 == 3) {
		 System.out.println("");
	 } else if (r1 == 4) {
			 System.out.println("야옹");
	 }	 else if (r1 == 5) {
				 System.out.println("야옹");
	 }else if (r1 == 6) {
		 System.out.println("야옹");
	 }
 }

}
}
